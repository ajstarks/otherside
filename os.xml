	<slide>
	      <text xp="5" yp="90" sp="4">Imports</text>
	<text xp="5" yp="80" sp="2.5" font="mono">import (
      "encoding/xml"
      "flag"
      "fmt"
      "io"
      "os"

      "github.com/ajstarks/svgo"
)</text>
	</slide>
	
	
	  <slide>
	      <text xp="5" yp="90" sp="4">The main</text>
	  	<text xp="5" yp="80" sp="2.5" font="mono">func main() {
      flag.Parse()
      for _, f := range flag.Args() {
          canvas.Start(*width, *height)
          dothing(f)
          canvas.End()
      }
}</text>
	  </slide>	


	<slide>
	<text xp="5" yp="90" sp="4">Define the input data structures</text>
	<text xp="5" yp="20" sp="1.6" color="rgb(100,100,100)" file="thing.xml"/>
	<text xp="5" yp="80" sp="2.5" font="mono">type Thing struct {
      Top  int `xml:"top,attr"`
      Left int `xml:"left,attr"`
      Sep  int `xml:"sep,attr"`
      Item []item `xml:"item"`
}
type item struct {
      Width  int    `xml:"width,attr"`
      Height int    `xml:"height,attr"`
      Name   string `xml:"name,attr"`
      Color  string `xml:"color,attr"`
      Text   string `xml:",chardata"`
}
</text>
	  </slide>
	  
	  <slide>
	<text xp="5" yp="90" sp="4">Define flags and output destination</text>
    <text xp="5" yp="80" sp="2.5" font="mono">var (
      width  = flag.Int("w", 1024, "width")
      height = flag.Int("h", 768, "height")
      canvas = svg.New(os.Stdout)
)</text>
	</slide>

	  
	  <slide>
	  	<text xp="5" yp="90" sp="4">Read the input</text>
          <text xp="5" yp="80" sp="2.5" font="mono">func dothing(location string) {
      f, err := os.Open(location)
      if err != nil {
          fmt.Fprintf(os.Stderr, "%v\n", err)
          return
      }
      defer f.Close()
      readthing(f)
}
</text>
	  
	  </slide>

	  <slide>
	  	<text xp="5" yp="90" sp="4">Parse and load the data structures</text>
	  	<text xp="5" yp="80" sp="2.5" font="mono">func readthing(r io.Reader) {
      var t Thing
      if err := xml.NewDecoder(r).Decode(&amp;t); err != nil {
          fmt.Fprintf(os.Stderr, "%v\n", err)
          return
      }
      drawthing(t)
}</text>
	  </slide>
	  
	  
	  <slide>
	  	<text xp="5" yp="90" sp="4">Use the items of "thing" to make the picture</text>
	  	<text xp="5" yp="80" sp="2.5" font="mono">func drawthing(t Thing) {
      x := t.Left
      y := t.Top
      for _, v := range t.Item {
          style := fmt.Sprintf("font-size:%dpx;fill:%s", 
                               v.Width/2, v.Color)
          canvas.Circle(x, y, v.Height/4, "fill:"+v.Color)
          canvas.Text(x+t.Sep, y, 
                      v.Name+":"+v.Text+"/"+v.Color, style)
          y += v.Height
      }
}</text>
